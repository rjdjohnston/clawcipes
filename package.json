{
  "name": "@jiggai/recipes",
  "version": "0.2.17",
  "description": "ClawRecipes plugin for OpenClaw (markdown recipes -> scaffold agents/teams)",
  "main": "index.ts",
  "type": "commonjs",
  "openclaw": {
    "extensions": [
      "./index.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.ts",
    "src/",
    "openclaw.plugin.json",
    "recipes/",
    "docs/",
    "clawcipes_cook.jpg",
    "README.md"
  ],
  "scripts": {
    "test": "vitest run",
    "test:smoke": "node scripts/scaffold-smoke.mjs",
    "smoke": "node scripts/scaffold-smoke.mjs",
    "scaffold:smoke": "node scripts/scaffold-smoke.mjs"
  },
  "keywords": [
    "openclaw",
    "clawrecipes",
    "plugin",
    "recipes"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sinclair/typebox": "^0.34.48",
    "json5": "^2.2.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "vitest": "^3.2.4"
  }
}
